
#init
npm init -y

#webpack
npm install webpack webpack-cli copy-webpack-plugin --save-dev

#ESLint
npm install eslint --save-dev -g
npx eslint --init

#Babel
npm install @babel/runtime @babel/core @babel/cli @babel/preset-env babel-loader --save-dev

#React
npm install react react-dom --save
npm install @babel/preset-react --save-dev

#SCSS
npm install node-sass --save
npm install style-loader css-loader sass-loader --save-dev
npm install mini-css-extract-plugin --save-dev

#Webpack-dev-server
npm install webpack-dev-server --save-dev

#Redux
npm install --save redux react-redux --legacy-peer-deps

